{#
/**
 * @file
 * Theme override to display a media item.
 *
 * Available variables:
 * - name: Name of the media.
 * - content: Media content.
 *
 * @see template_preprocess_media()
 *
 * @ingroup themeable
 */
#}
{% if title_suffix is not empty or attributes['data-quickedit-entity-id'] %}
<div{{ attributes }}>
  {{ title_suffix }}
{% endif %}
  {% if content %}
    {{ content }}
  {% endif %}
{% if title_suffix is not empty or attributes['data-quickedit-entity-id'] %}
</div>
{% endif %}
