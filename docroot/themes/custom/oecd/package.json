{
  "name": "@space-designsystem/starter-kit-tailwind2",
  "version": "1.1.7",
  "description": "A design system with Storybook, Tailwind and Drupal.",
  "main": "",
  "license": "GPL-2.0",
  "author": {
    "name": "Saket Kumar",
    "url": "https://github.com/spacedesignsystem/space"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:qed42/spacedesignsystem/space.git"
  },
  "keywords": [
    "storybook",
    "ui_patterns",
    "prototyping"
  ],
  "scripts": {
    "dev:storybook": "start-storybook --config-dir apps/storybook",
    "dev:storybook:docksal": "start-storybook --config-dir apps/storybook -p 3000",
    "build:storybook": "build-storybook --config-dir apps/storybook -o ./dist/app-storybook",
    "docs:storybook": "build-storybook --docs --config-dir apps/storybook -o ./dist/app-storybook",
    "build-storybook": "build-storybook --config-dir apps/storybook",
    "deploy:storybook": "storybook-to-ghpages --existing-output-dir ./dist/app-storybook",
    "dev:drupal": "npx token-transformer source/figma/tokens.json SPACEtokens.json && cross-env-shell NODE_ENV=development \"webpack --watch --config ./apps/drupal/webpack.config.js\"",
    "build:drupal": "npx token-transformer source/figma/tokens.json SPACEtokens.json && cross-env-shell NODE_ENV=production \"webpack  --config ./apps/drupal/webpack.config.js\" && esbuild --outfile=../../../sw.js --bundle sw.ts && workbox injectManifest",
    "lint:js": "eslint --ext .js ./",
    "lint:css": "stylelint '**/*.css'",
    "lint": "npm run lint:js; npm run lint:css;",
    "fmt:js": "eslint --fix --ext .js,.vue ./",
    "fmt:css": "npm run lint:css --fix && prettier --loglevel warn --write '**/*.{css}'",
    "fmt": "npm run fmt:js; npm run fmt:css",
    "ci": "npm run lint && npm run build:drupal"
  },
  "dependencies": {
    "@drupal/once": "^1.0.1",
    "@glidejs/glide": "^3.4.1",
    "@splidejs/splide": "^2.4.21",
    "@storybook/addon-a11y": "^6.3.7",
    "@storybook/theming": "^6.3.7",
    "@tailwindcss/forms": "^0.5.2",
    "@tailwindcss/typography": "^0.5.4",
    "alpinejs": "^2.7.0",
    "counterup2": "^2.0.2",
    "critical": "^5.1.1",
    "esm": "^3.2.25",
    "jquery": "^3.6.0",
    "jquery-once": "^2.2.3",
    "magnific-popup": "^1.1.0",
    "puppeteer": "^20.8.3",
    "slick-slider": "^1.8.2",
    "svg4everybody": "^2.1.9",
    "waypoints": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@space-designsystem/cli": "^1.1.1-beta",
    "@space-designsystem/core": "^1.0.5",
    "@space-designsystem/pattern": "^1.0.5",
    "@space-designsystem/preset-postcss8": "^1.1.7",
    "@space-designsystem/preset-tailwind2": "^1.0.5",
    "@space-designsystem/storybook": "^1.0.5",
    "@storybook/addon-links": "^6.3.7",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/addon-viewport": "^6.1.0",
    "@storybook/addons": "^6.1.0",
    "@storybook/linter-config": "^2.5.0",
    "@storybook/react": "^6.1.0",
    "@storybook/storybook-deployer": "^2.8.7",
    "@tailwindcss/jit": "^0.1.18",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "autoprefixer": "^10.4.14",
    "browserslist": "^4.22.2",
    "core-js": "^3.4.1",
    "cross-env": "^6.0.3",
    "cssnano": "^4.1.11",
    "esbuild": "^0.18.4",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-webpack": "^0.12.0",
    "eslint-loader": "^3.0.3",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jest": "^23.7.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^6.0.1",
    "lodash": "^4.17.20",
    "postcss": "^8.4.26",
    "postcss-nested": "^4.2.1",
    "postcss-preset-env": "^9.3.0",
    "postcss-rtlcss": "^3.6.3",
    "prettier": "^1.19.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "regenerator-runtime": "^0.13.3",
    "require-context": "^1.1.0",
    "storybook-addon-themes": "^6.0.1",
    "stylelint-scss": "^3.17.2",
    "tailwindcss": "^3.3.3",
    "webpack": "^4.41.3",
    "workbox-cli": "^7.0.0",
    "workbox-precaching": "^7.0.0",
    "yo": "^4.3.0"
  },
  "engines": {
    "node": "20.11.0",
    "npm": "9.6.1"
  }
}
