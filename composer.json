    "config": {
        "process-timeout": 0,
        "allow-plugins": {
            "cweagans/composer-patches": true,
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "drupal/core-vendor-hardening": true,
            "acquia/blt": true,
            "drupal/console-extend-plugin": true,
            "oomphinc/composer-installers-extender": true,
            "mglaman/composer-drupal-lenient": true
        },
        "platform": {
            "php": "8.1"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "patches": {
            "drupal/lightning_layout": {
                "Call to a member function uuid() on null when Layout Builder renders a Placeholder": "https://www.drupal.org/files/issues/2020-04-15/light_layout.patch"
            },
            "drupal/component_blocks": {
                "3210538 - Not working with Settings Type url and token ": "https://git.drupalcode.org/project/component_blocks/-/merge_requests/2.patch"
            },
            "drupal/core": {
                "Not all form elements can't use in vertical_tabs": "https://www.drupal.org/files/issues/2023-10-17/3122920_group_patch_17-reroll-for-10.1.patch",
                "PHP message: Error: Call to a member function getEntityTypeId() on null (Layout Builder)": "https://www.drupal.org/files/issues/2020-08-05/3049332-inline-35.patch",
                "Stream wrapper reference in JS library causes error in _locale_parse_js_file()": "https://www.drupal.org/files/issues/2020-05-08/2735717-28.patch",
                "New non translatable field on translatable content throws error": "https://www.drupal.org/files/issues/2021-02-10/3025039_42.patch",
                "2869592 - Disabled update module shouldn't produce a status report warning": "https://www.drupal.org/files/issues/2020-02-07/2869592-remove-update-warning-34.patch",
                "Call to a member function delete() on null": "https://www.drupal.org/files/issues/2021-10-27/error-running-config-import-delete-on-null-3198715-8.patch",
                "TranslatableMarkup for Invalid Argument Exception: $string (Array) must be a string.": "https://www.drupal.org/files/issues/2021-07-30/2719553-42_0.patch",
                "Layout Builder doesn't respect the view mode set in a hook_entity_view_mode_alter() implementation":"https://www.drupal.org/files/issues/2019-09-26/3083754-4.patch",
                "MenuLinkContentAccessControlHandler does not allow 'view' access without admin permission": "https://git.drupalcode.org/project/drupal/-/merge_requests/339.diff",
                "Drag-and-drop reordering doesn't work if the region content is nested": "https://www.drupal.org/files/issues/2023-01-26/3062742-26.patch",
                "strnatcasecmp(): Passing null to parameter #2 ($string2) of type string is deprecated": "https://www.drupal.org/files/issues/2022-11-17/3315678-11_0.patch"
            },
            "drupal/layout_builder_st": {
                "Argument 1 passed to Drupal\\layout_builder_st\\EventSubscriber\\ComponentPluginTranslate::isTranslation() must implement interface Drupal\\layout_builder\\SectionStorageInterface, null given": "https://www.drupal.org/files/issues/2019-07-24/isTranslation-null-given.patch",
                "Inline block loading active block_content revision even if serialized block exists\n": "https://www.drupal.org/files/issues/2020-05-15/layout_builder_st-3067646-10.patch"
            },
            "drupal/section_library": {
                "#3228163: Add create or update logic.": "https://www.drupal.org/files/issues/2022-12-09/section-library-3228163-10-refactored.patch"
            },
            "drupal/drimage": {
                "Stage file proxy": "https://git.drupalcode.org/project/drimage/-/merge_requests/1.patch"
            },
            "drupal/ui_patterns": {
                "Allow for Layout Builder sections to be given administrativ.": "https://patch-diff.githubusercontent.com/raw/nuvoleweb/ui_patterns/pull/278.patch",
                "Add quick edit": "https://patch-diff.githubusercontent.com/raw/nuvoleweb/ui_patterns/pull/273.patch",
                "PatternDefinitionField constructor to default label to name if label does not exist": "https://patch-diff.githubusercontent.com/raw/nuvoleweb/ui_patterns/pull/351.patch"
            },
            "drupal/fivestar": {
                "Relying on Entity Queries to check access by default is depre": "https://www.drupal.org/files/issues/2023-03-15/relyingOnEntityQuery-3348074-1.patch",
                "Remove 'fivestar_preview' theme function": "https://www.drupal.org/files/issues/2023-10-01/3273701_36.patch"
            },
            "drupal/ui_patterns_views_style" : {
                "Pattern settings is not available in the Views": "https://www.drupal.org/files/issues/2022-07-05/pattern-format-settings-3294280-2.patch"
            },
            "drupal/layout_builder_modal": {
              "Solved layout block translation issue": "https://www.drupal.org/files/issues/2022-12-15/3133695-13.patch"
            },
            "drupal/node_type_class": {
              "Drupal 10 compatibility": "https://www.drupal.org/files/issues/2022-06-16/node_type_class.2.0.1.rector.patch"
            },
            "drupal/checklist_security": {
                "Drupal 10 compatibility": "https://www.drupal.org/files/issues/2022-06-15/checklist_security.1.0.x-dev.rector.patch"
            },
            "drupal/toolbar_anti_flicker": {
                "Drupal 10 compatibility": "https://www.drupal.org/files/issues/2023-06-14/toolbar-anti-flicker-d10-3299332_0.patch",
                "Solved console error for $(...).once": "https://www.drupal.org/files/issues/2023-04-25/3356226-fix-jquery-once-3.patch"
            }
        },
        "composer-exit-on-patch-failure": true,
        "drupal-scaffold": {
            "locations": {
                "web-root": "docroot/"
            }
        },
        "enable-patching": true,
        "installer-paths": {
            "docroot/core": [
                "type:drupal-core"
            ],
            "docroot/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "docroot/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "docroot/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "docroot/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "docroot/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "docroot/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "installer-types": [
            "drupal-profile",
            "drupal-module",
            "drupal-core",
            "drupal-library",
            "bower-asset",
            "npm-asset"
        ],
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "drupal-lenient": {
            "allowed-list": [
                "drupal/colorwidget",
                "drupal/node_type_class",
                "drupal/toolbar_anti_flicker",
                "drupal/checklist_security"
            ]
        }
    }
}
