{
    "name": "qed42/drupal-vanilla-baseline",
    "description": "QED42 Project template for Drupal 9 projects.",
    "type": "template",
    "license": "GPL-2.0-or-later",
    "homepage": "https://github.com/qed42/drupal-vanilla-baseline",
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        },
        {
            "name": "qed42/qed42ds",
            "type": "vcs",
            "url": "https://github.com/qed42/qed42ds"
        },
        {
            "name": "qed42/paragraphs_baseline",
            "type": "vcs",
            "url": "https://github.com/qed42/paragraphs_baseline"
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "drupal/core-composer-scaffold": "^9.2",
        "drupal/core-project-message": "^9.2",
        "drupal/core-recommended": "^9.2",
        "drupal/allowed_formats": "^1.3",
        "drupal/color_field": "^2.4",
        "drupal/admin_toolbar": "^2.1",
        "drupal/autofill": "^1.0",
        "drupal/autosave_form": "^1.2",
        "drupal/blazy": "^2.0",
        "drupal/config_update": "^1.7",
        "drupal/content_lock": "^2.1",
        "drupal/crop": "^2.1",
        "drupal/ctools": "^3.4",
        "drupal/diff": "1.0",
        "drupal/dropzonejs": "^2.1",
        "drupal/entity_browser": "2.6",
        "drupal/entity_reference_actions": "^1.0-beta5",
        "drupal/entity_reference_revisions": "^1.3",
        "drupal/field_group": "3.1",
        "drupal/focal_point": "^1.3",
        "drupal/facets": "^1.4",
        "drupal/google_analytics": "^2.0",
        "drupal/inline_entity_form": "^1.0-rc7",
        "drupal/length_indicator": "^1.1",
        "drupal/linkit": "^6.0.0-beta1",
        "drupal/media_entity_instagram": "^3.0.3",
        "drupal/media_entity_pinterest": "^2.0",
        "drupal/media_entity_slideshow": "^2.0-alpha1",
        "drupal/media_entity_twitter": "^2.5",
        "drupal/media_expire": "^2.4",
        "drupal/metatag": "^1.13",
        "drupal/metatag_async_widget": "^1.0-alpha2",
        "drupal/password_policy": "3.0",
        "drupal/pathauto": "^1.8",
        "drupal/redirect": "^1.6",
        "drupal/scheduler": "^1.3",
        "drupal/scheduler_content_moderation_integration": "^1.3",
        "drupal/select2": "^1.7",
        "drupal/search_api_mark_outdated": "^1.0",
        "drupal/simple_sitemap": "^3.10",
        "drupal/shariff": "^1.5",
        "drupal/slick": "^2.2",
        "drupal/token": "^1.7",
        "drupal/ace_editor": "^1.3",
        "drupal/image_effects": "^3.1",
        "drupal/svg_image": "^1.15",
        "drupal/search_api": "^1.17",
        "drupal/paragraphs": "^1.12",
        "drupal/classy_paragraphs": "^1.0@beta",
        "drupal/layout_paragraphs": "^1",
        "drupal/paragraphs_ee": "^1.7",
        "drupal/paragraphs_features": "^1.11",
        "drupal/paragraphs_previewer": "^1.5",
        "drupal/component_blocks": "^1.2",
        "drupal/components": "^2.4",
        "drupal/ds": "^3.13",
        "drupal/twig_tweak": "^3.1",
        "drupal/ui_patterns": "^1.2",
        "drupal/wingsuit_companion": "^1.0",
        "drupal/ckeditorheight": "^1.8",
        "drupal/ckeditor_media_embed": "^1.10",
        "drupal/codesnippet": "^1.7",
        "drupal/extlink": "^1.6",
        "drupal/imageapi_optimize": "^4.0.0",
        "drush/drush": "^9.0||^10.0",
        "oomphinc/composer-installers-extender": "^2.0",
        "vlucas/phpdotenv": "^2.4",
        "qed42/qed42ds": "dev-main",
        "qed42/paragraphs_baseline": "dev-main",
        "npm-asset/blazy": "^1.8",
        "npm-asset/slick-carousel": "^1.8",
        "npm-asset/shariff": "^3.1.1",
        "npm-asset/select2": "^4.0.7",
        "npm-asset/dropzone": "^5.5.1",
        "npm-asset/exif-js": "^2.3.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "patches": {
            "drupal/diff": {
                "Back button for comparison page": "https://www.drupal.org/files/issues/back_button_for-2853193-4.patch"
            },
            "drupal/entity_browser": {
                "Provide inline entity form FieldWidgetDisplay": "https://www.drupal.org/files/issues/2019-07-04/2858438_17.patch",
                "Re-order + remove broken with the Entity Reference (and File) widget": "https://www.drupal.org/files/issues/2020-06-05/2851580-88.patch",
                "Trying to access array offset on value of type null in EntityBrowserElement valueCallback": "https://www.drupal.org/files/issues/2021-02-19/3199269-array-value-null.patch"
            },
            "drupal/password_policy": {
                "Policies should declare a config dependency on the plugin modules": "https://www.drupal.org/files/issues/2020-04-14/2918974-7-11.patch"
            },
            "drupal/field_group": {
                "Undefined property: stdClass::$region in field_group_form_process()": "https://www.drupal.org/files/issues/2020-06-15/3059614-37.patch"
            },
            "drupal/simple_sitemap": {
                "Sitemap variant index functionality": "https://www.drupal.org/files/issues/2021-03-03/simple_sitemap-sitemap_variant_index-3109090-16.patch"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, initial setup for the Drupal project is complete  </>",
                "<bg=blue;fg=white>  from the qed42/qed42_vanilla_drupal template!          </>",
                "<bg=blue;fg=white>                                                         </>"
            ]
        }
    }
}
